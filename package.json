{
  "name": "autoflash",
  "version": "2017.6.11-1",
  "description": "Autoflasher for E3372",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "install": "electron-rebuild",
    "build": "electron-rebuild --arch=ia32 && electron-packager ./ autoflash --platform=win32 --arch=ia32 --icon=../autoflash.ico --out=../ --asar.unpack=\"{*.exe,*.bin}\" && electron-rebuild"
  },
  "repository": "https://github.com/ilya-fedin/autoflash",
  "author": "ilya-fedin",
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-packager": "^8.7.0",
    "electron-rebuild": "^1.5.11"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "iconv-lite": "^0.4.17",
    "jquery": "^3.2.1",
    "node-wmi": "0.0.4"
  }
}
